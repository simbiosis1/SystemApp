<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:e="urn:import:org.simbiosis.systemui.client.editor"
	xmlns:ke="urn:import:org.kembang.module.client.editor"
	>
	<g:HTMLPanel>
		<table>
			<tr>
				<td class="formLabel">Nama user</td>
				<td>
					<g:TextBox ui:field="name" styleName="formText" />
				</td>
			</tr>
			<tr>
				<td class="formLabel">Nama lengkap</td>
				<td>
					<g:TextBox ui:field="realName" styleName="formText" />
				</td>
			</tr>
			<tr>
				<td class="formLabel">Email</td>
				<td>
					<g:TextBox ui:field="email" styleName="formText" />
				</td>
			</tr>
			<tr>
				<td class="formLabel">Hak akses</td>
				<td>
					<e:RoleListBox ui:field="role" styleName="formCombobox" />
				</td>
			</tr>
			<tr>
				<td class="formLabel">Cabang</td>
				<td>
					<ke:BranchListBox ui:field="branch" styleName="formCombobox" />
				</td>
			</tr>
			<tr>
				<td class="formLabel">Kas</td>
				<td>
					<e:SubBranchListBox ui:field="subBranch"
						styleName="formCombobox" />
				</td>
			</tr>
			<tr>
				<td class="formLabel">Level</td>
				<td>
					<e:LevelTypeEditor ui:field="level" styleName="formCombobox" />
				</td>
			</tr>
			<tr>
				<td colspan="2">
					<g:CheckBox ui:field="active" styleName="formCheckbox" />
					Aktif
				</td>
			</tr>
			<tr>
				<td colspan="2">
					<g:CheckBox ui:field="changePassword" styleName="formCheckbox" />
					Ubah kata kunci
				</td>
			</tr>
			<tr>
				<td class="formLabel">Kata kunci</td>
				<td>
					<g:PasswordTextBox ui:field="password" styleName="formText" />
				</td>
			</tr>
			<tr>
				<td class="formLabel">Konfirmasi kata kunci</td>
				<td>
					<g:PasswordTextBox ui:field="confirmPassword"
						styleName="formText" />
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 