<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:ke="urn:import:org.kembang.module.client.editor"
	>
	<g:HTMLPanel>
		<table>
			<tr>
				<td class="formLabel">Kode</td>
				<td>
					<g:TextBox ui:field="code" styleName="formText" />
				</td>
			</tr>
			<tr>
				<td class="formLabel">Nama</td>
				<td>
					<g:TextBox ui:field="name" styleName="formText" />
				</td>
			</tr>
			<tr>
				<td class="formLabel">Alamat</td>
				<td>
					<g:TextBox ui:field="address" styleName="formText" />
				</td>
			</tr>
			<tr>
				<td class="formLabel">Cabang</td>
				<td>
					<ke:BranchListBox ui:field="branch" styleName="formCombobox" />
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 