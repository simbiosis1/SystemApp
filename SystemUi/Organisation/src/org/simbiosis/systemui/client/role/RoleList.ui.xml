	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:e="urn:import:org.simbiosis.systemui.client.editor"
	>
	<g:DockLayoutPanel unit="PX">
		<g:west size="250">
			<e:RolesTable ui:field="roles"/>
